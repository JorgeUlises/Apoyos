@*---------Modal Muestra Detalle de Publicidad ---------*@

<div class="modal fade bd-example-modal-xl" id="modalMuestraDatosPublicidad" tabindex="-1" role="dialog" aria-labelledby="ModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl sombraModal" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span class='far fa-times-circle fa-lg' title='Cerrar'></span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-4 col-md-3 col-xxl-3">
                        <div class="card">
                            <div id='FotoPersonaPub'></div>
                            <div class="card-body">
                                <div id='NombrePersonaPub'></div>
                            </div>
                            <div class="card-footer">
                                <div id='DatosPersonaPub'></div>
                            </div>
                        </div>
                    </div>
                    <div class="col-8 col-md-6 col-xxl-3 col-sm-3">
                        <div class="col">
                            <div class='container-fluid'>
                                <div class="row row-cols-5">
                                    <div id="divCarrouselFotosPublicidad"></div>
                                </div>
                            </div>
                            <br />
                            <div id="divFotoAmpliada" style="align-content:center;"></div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn BotonNaranjaClaro" data-dismiss="modal">Cerrar</button>
                </div>
            </div>
        </div>
    </div>
</div>