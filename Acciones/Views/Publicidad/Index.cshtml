@model IEnumerable<Acciones.Models.Publicidad>

@{
    ViewData["Title"] = "Index";
}

<script type="text/javascript">
    document.addEventListener("DOMContentLoaded", function (event) {
        @Html.Raw(ViewBag.JavaScriptFunction)
    });

</script>

<link href="@Url.Content("~/css/mapa.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("//cdnjs.cloudflare.com/ajax/libs/ol3/3.5.0/ol.min.css")" rel="stylesheet" type="text/css" />



